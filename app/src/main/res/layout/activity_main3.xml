<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2"
    android:background="#420505">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="290dp"
        android:layout_marginTop="32dp"
        android:paddingVertical="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/startingpage" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="254dp"
        android:layout_height="102dp"
        android:layout_marginTop="-20dp"
        android:fontFamily="@font/qwitchergrypenregular"

        android:padding="0sp"


        android:text="Chanchal"
        android:textAlignment="center"
        android:textColor="#eeeee4"
        android:textSize="90sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="254dp"
        android:layout_height="102dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/qwitchergrypenregular"

        android:padding="0sp"


        android:text="Ornaments"
        android:textAlignment="center"
        android:textColor="#eeeee4"
        android:textSize="90sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.898"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/start_to_signup"
        android:layout_width="300dp"
        android:layout_height="54dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/rounded_button"
        android:fontFamily="@font/intersimple"
        android:padding="0sp"
        android:text="Get Started"
        android:textAlignment="center"

        android:textColor="#ffffff"
        android:textSize="29sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="332dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:text="@string/by_clicking_on_start_you_agree_to_all_the_terms_and_conditions"
        android:textAlignment="center"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/start_to_signup" />


</androidx.constraintlayout.widget.ConstraintLayout>